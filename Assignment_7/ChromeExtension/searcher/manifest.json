{
  "manifest_version": 3,
  "name": "Search Query Opener",
  "version": "1.0",
  "description": "Search a query and open the result in a new tab",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
